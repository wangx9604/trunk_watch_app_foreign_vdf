package com.xiaoxun.xun.motion.beans;

public class RecordDetailBean {
    int mHeadIconId;
    String mTitle;
    String mValue;
    String mUnit;

    public RecordDetailBean(int mHeadIconId, String mTitle, String mValue, String mUnit) {
        this.mHeadIconId = mHeadIconId;
        this.mTitle = mTitle;
        this.mValue = mValue;
        this.mUnit = mUnit;
    }

    public int getmHeadIconId() {
        return mHeadIconId;
    }

    public void setmHeadIconId(int mHeadIconId) {
        this.mHeadIconId = mHeadIconId;
    }

    public String getmTitle() {
        return mTitle;
    }

    public void setmTitle(String mTitle) {
        this.mTitle = mTitle;
    }

    public String getmValue() {
        return mValue;
    }

    public void setmValue(String mValue) {
        this.mValue = mValue;
    }

    public String getmUnit() {
        return mUnit;
    }

    public void setmUnit(String mUnit) {
        this.mUnit = mUnit;
    }
}
