package dx.client.api;

/**
 * Default implementation for IEndpointListener interface. if you want to callback your code for a method, just override it.
 * @author Sean
 *
 */
public class DefaultEndpointListener implements IEndpointListener{

	@Override
	public void onOpen(int status, String reason,int port) {
	}

	@Override
	public void onClose(int status, String reason, int port) {
	}

	@Override
	public void onReceive(byte[] data) {
	}

    @Override
    public void onReceive(String message,int prot) {
    }

    @Override
    public void onPing() {
    }

    
	@Override
	public void onError(int errorCode, String reason, int port) {
	}

}
