package com.xiaoxun.xun.health.outside.bean;

public class OutSideChartBean {
    int lvl1_dura = 0;  //1级持续时间
    int lvl2_dura = 0;  //2级持续时间
    int lvl3_dura = 0;  //3级持续时间
    int lvl4_dura = 0;  //4级持续时间
    int lvl5_dura = 0;  //5级持续时间

    public OutSideChartBean(){}

    public void addLvl1_dura(int dura) {
        this.lvl1_dura += dura;
    }

    public int getLvl1_dura() {
        return lvl1_dura;
    }

    public void addLvl2_dura(int dura) {
        this.lvl2_dura += dura;
    }

    public int getLvl2_dura() {
        return lvl2_dura;
    }

    public void addLvl3_dura(int dura) {
        this.lvl3_dura += dura;
    }

    public int getLvl3_dura() {
        return lvl3_dura;
    }

    public void addLvl4_dura(int dura) {
        this.lvl4_dura += dura;
    }

    public int getLvl4_dura() {
        return lvl4_dura;
    }

    public void addLvl5_dura(int dura) {
        this.lvl5_dura += dura;
    }

    public int getLvl5_dura() {
        return lvl5_dura;
    }

    public int getAllDuration(){
        return (lvl1_dura + lvl2_dura + lvl3_dura + lvl4_dura + lvl5_dura);
    }
}
