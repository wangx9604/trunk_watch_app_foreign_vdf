<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true">

    <include
        android:id="@+id/layout_title"
        layout="@layout/layout_title_and_back" />

    <TextView
        android:id="@+id/tv_agreement_and_privacy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25.33dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="25.33dp"
        android:text="@string/agreement_and_privacy"
        android:textColor="@color/alipay_333333"
        android:textSize="16.33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_title" />

    <TextView
        android:id="@+id/tv_agreement_and_privacy_tips"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25.33dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="25.33dp"
        android:text="@string/agreement_and_privacy_tips"
        android:textColor="#999999"
        android:textSize="13.33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_agreement_and_privacy" />

    <Button
        android:id="@+id/btn_disagree"
        android:layout_width="0dp"
        android:layout_height="40.33dp"
        android:layout_marginStart="26.33dp"
        android:layout_marginEnd="26.33dp"
        android:layout_marginBottom="13.33dp"
        android:background="@drawable/disagree_background"
        android:text="@string/disagree"
        android:textColor="#33A72A"
        android:textSize="13.33sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_agree"
        android:layout_width="0dp"
        android:layout_height="40.33dp"
        android:layout_marginStart="26.33dp"
        android:layout_marginEnd="26.33dp"
        android:layout_marginBottom="13.33dp"
        android:background="@drawable/agree_background"
        android:text="@string/bind_ask_accept"
        android:textColor="@color/white"
        android:textSize="13.33sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_disagree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>