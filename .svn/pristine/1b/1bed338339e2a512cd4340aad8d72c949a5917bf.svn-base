<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<androidx.cardview.widget.CardView
		android:id="@+id/cardview_week"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginStart="7.5dp"
		android:layout_marginEnd="7.5dp"
		app:cardCornerRadius="12.5dp"
		app:layout_constraintTop_toTopOf="parent">
		<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<TextView
				android:id="@+id/tv_week_1"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:layout_marginTop="10dp"
				android:text="@string/week_fn_1"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toTopOf="parent"/>
			<ImageView
				android:id="@+id/iv_week_selector_1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_1"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_1"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_1"/>
			<TextView
				android:id="@+id/tv_week_2"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/week_fn_2"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_1"/>
			<ImageView
				android:id="@+id/iv_week_selector_2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_2"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_2"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_2"/>
			<TextView
				android:id="@+id/tv_week_3"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/week_fn_3"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_2"/>
			<ImageView
				android:id="@+id/iv_week_selector_3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_3"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_3"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_3"/>
			<TextView
				android:id="@+id/tv_week_4"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/week_fn_4"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_3"/>
			<ImageView
				android:id="@+id/iv_week_selector_4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_4"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_4"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_4"/>
			<TextView
				android:id="@+id/tv_week_5"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/week_fn_5"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_4"/>
			<ImageView
				android:id="@+id/iv_week_selector_5"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_5"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_5"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_5"/>
			<TextView
				android:id="@+id/tv_week_6"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/week_fn_6"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_5"/>
			<ImageView
				android:id="@+id/iv_week_selector_6"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_6"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_6"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				android:layout_marginStart="25dp"
				android:layout_marginEnd="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_6"/>
			<TextView
				android:id="@+id/tv_week_7"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/week_fn_7"
				android:textSize="17sp"
				android:textColor="@color/black"
				android:gravity="center_vertical"
				android:paddingStart="25dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_6"/>
			<ImageView
				android:id="@+id/iv_week_selector_7"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="25dp"
				android:background="@drawable/calendar_week_selector"
				app:layout_constraintTop_toTopOf="@+id/tv_week_7"
				app:layout_constraintBottom_toBottomOf="@+id/tv_week_7"
				app:layout_constraintEnd_toEndOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="10dp"
				app:layout_constraintTop_toBottomOf="@+id/tv_week_7"/>

		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.cardview.widget.CardView>
	<androidx.cardview.widget.CardView
		android:layout_width="match_parent"
		android:layout_height="55dp"
		android:layout_marginTop="8dp"
		android:layout_marginStart="7.5dp"
		android:layout_marginEnd="7.5dp"
		app:cardCornerRadius="12.5dp"
		app:layout_constraintTop_toBottomOf="@+id/cardview_week">
		<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent">
			<View
				android:id="@+id/view_divi_line"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:background="@color/item_grey"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"/>
			<TextView
				android:id="@+id/tv_cancel"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:text="@string/cancel"
				android:textColor="@color/heart_rate_warning_value"
				android:textSize="19sp"
				android:gravity="center"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="@+id/view_divi_line"/>

			<TextView
				android:id="@+id/tv_confirm"
				android:layout_width="0dp"
				android:layout_height="match_parent"
				android:text="@string/confirm"
				android:textColor="@color/heart_rate_warning_value"
				android:textSize="19sp"
				android:textStyle="bold"
				android:gravity="center"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintStart_toStartOf="@+id/view_divi_line"/>
		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>

