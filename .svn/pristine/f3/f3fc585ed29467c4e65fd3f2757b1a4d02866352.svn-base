<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<androidx.cardview.widget.CardView
		android:id="@+id/cardview_week"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginStart="7.5dp"
		android:layout_marginEnd="7.5dp"
		app:cardCornerRadius="12.5dp"
		app:layout_constraintTop_toTopOf="parent">
		<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<TextView
				android:id="@+id/tv_action_1"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:layout_marginTop="10dp"
				android:text="@string/pulic_save_drafts"
				android:textSize="17sp"
				android:textColor="@color/heart_rate_warning_value"
				android:gravity="center"
				app:layout_constraintTop_toTopOf="parent"/>
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/item_grey"
				app:layout_constraintTop_toBottomOf="@+id/tv_action_1"/>
			<TextView
				android:id="@+id/tv_action_2"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:text="@string/pulic_giveup"
				android:textSize="17sp"
				android:textColor="@color/heart_rate_warning_value"
				android:gravity="center"
				app:layout_constraintTop_toBottomOf="@+id/tv_action_1"/>


		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.cardview.widget.CardView>
	<androidx.cardview.widget.CardView
		android:layout_width="match_parent"
		android:layout_height="55dp"
		android:layout_marginTop="8dp"
		android:layout_marginStart="7.5dp"
		android:layout_marginEnd="7.5dp"
		app:cardCornerRadius="12.5dp"
		app:layout_constraintTop_toBottomOf="@+id/cardview_week">
		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/layout_cancle"
			android:layout_width="match_parent"
			android:layout_height="match_parent">
			<TextView
				android:id="@+id/tv_cancel"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="@string/cancel"
				android:textStyle="bold"
				android:textColor="@color/heart_rate_warning_value"
				android:textSize="19sp"
				android:gravity="center"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintBottom_toBottomOf="parent"/>

		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>

