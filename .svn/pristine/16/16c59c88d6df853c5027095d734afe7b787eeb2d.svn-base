<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/tv_thing_2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:text="@string/calendar_thing_2"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="6dp"
        android:gravity="center"
        android:background="@drawable/rectangle_grey"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/rectangle_orag_stroke"
        app:layout_constraintStart_toStartOf="@+id/tv_thing_2"
        app:layout_constraintEnd_toEndOf="@+id/tv_thing_2"
        app:layout_constraintTop_toTopOf="@+id/tv_thing_2"
        app:layout_constraintBottom_toBottomOf="@+id/tv_thing_2"/>
    <ImageView
        android:id="@+id/iv_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/things_select_close"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>