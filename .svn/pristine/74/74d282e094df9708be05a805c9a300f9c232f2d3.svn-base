<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:gravity="center"
        android:layout_centerVertical="true"
        >

        <RelativeLayout
            android:id="@+id/rout_migration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/divideleftandright"
            android:paddingRight="@dimen/divideleftandright"
            android:layout_marginTop="@dimen/divideleftandright"
            >
            <!--        <ImageView
                        android:id="@+id/iv_migration_warning"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/set_up_icon"
                        android:layout_centerVertical="true"
                        />-->
            <TextView
                android:id="@+id/tv_migration_warning"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_7"
                android:textSize="@dimen/xiaomiMyTextSize1"
                android:text="@string/migration_remarks"
                android:layout_marginLeft="5dp"
                />
            <!--<TextView
                android:id="@+id/tv_migration_zhuyi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_7"
                android:textSize="@dimen/xiaomiMyTextSize0"
                android:text="注意："
                android:layout_marginLeft="5dp"
                android:layout_below="@+id/tv_migration_warning"
                />-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_7"
                android:textSize="@dimen/xiaomiMyTextSize1"
                android:text="@string/migration_becareful"
                android:layout_marginLeft="5dp"
                android:layout_below="@+id/tv_migration_warning"
                />
        </RelativeLayout>

        <!--<GridView
            android:id="@+id/gridView_migration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnWidth="90dp"
            android:numColumns="3"
            android:verticalSpacing="10dp"
            android:horizontalSpacing="10dp"
            android:stretchMode="columnWidth"
            android:gravity="center"
            android:layout_below="@+id/rout_migration"
            android:layout_above="@+id/lout_migration"
            android:paddingLeft="@dimen/divideleftandright"
            android:paddingRight="@dimen/divideleftandright"
            />-->
        <GridView
            android:id="@+id/gridView_migration"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/rout_migration"
            android:columnWidth="90dp"
            android:gravity="center"
            android:horizontalSpacing="10dp"
            android:numColumns="3"
            android:paddingLeft="@dimen/divideleftandright"
            android:paddingRight="@dimen/divideleftandright"
            android:stretchMode="columnWidth"
            android:verticalSpacing="10dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/gridView_migration"
            android:paddingLeft="@dimen/divideleftandright"
            android:paddingRight="@dimen/divideleftandright"
            android:layout_marginBottom="@dimen/divideleftandright"
            >
            <Button
                android:id="@+id/btn_nomigrate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cancel"
                android:layout_weight="1"
                android:background="@drawable/btn_alarm_customer_week_cancel_select"
                />
            <Button
                android:id="@+id/btn_datamigrate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/confirm"
                android:layout_weight="1"
                android:background="@drawable/btn_alarm_customer_week_confirm_select"
                />

        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>