<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <include
        android:id="@+id/title"
        layout="@layout/activity_title"/>
    <TextView
        android:id="@+id/call_back_member_tips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/title"
        android:layout_marginLeft="23.33dp"
        android:layout_marginRight="23.33dp"
        android:layout_marginTop="13.33dp"
        android:text="@string/call_back_number_tips"
        android:textColor="@color/txt_grey"
        android:textSize="@dimen/xiaomiMyTextSize13"
        />
    <ImageView
        android:id="@+id/list_divide"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_below="@id/call_back_member_tips"
        android:layout_marginLeft="23.33dp"
        android:layout_marginRight="23.33dp"
        android:layout_marginTop="8.66dp"
        android:src="@color/line_grey"
        />
    <ListView
        android:id="@+id/phone_white_listview"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/list_divide"
        android:cacheColorHint="@color/transparent"
        android:divider="@drawable/list_item_divider"
        android:dividerHeight="1px"
        android:scrollbars="none"
        android:fadingEdge="none" />
</RelativeLayout>

