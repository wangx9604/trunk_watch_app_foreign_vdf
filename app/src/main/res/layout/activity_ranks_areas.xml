<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    tools:context="com.xiaoxun.xun.activitys.RanksAreasActivity">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="45.3dp"
        android:background="@color/bg_color_orange">
        <ImageButton
            android:id="@+id/iv_title_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="13.3dp"
            android:background="@drawable/btn_title_back_selector" />

        <LinearLayout
            android:id="@+id/ly_title"
            android:layout_width="wrap_content"
            android:layout_height="32.33dp"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="9dp"
            android:layout_marginTop="9.66dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_ranks_area"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ranks_area"
                android:textColor="@color/bg_color_orange"
                android:gravity="center"
                android:background="@drawable/switch_right_select"/>
            <TextView
                android:id="@+id/tv_ranks_country"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ranks_country"
                android:textColor="@color/white"
                android:gravity="center"
                android:background="@drawable/msgtitle_lablayout_selector"/>
        </LinearLayout>
    </RelativeLayout>

    <com.telecom.websdk.NormalWebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webView"
        />

</LinearLayout>
